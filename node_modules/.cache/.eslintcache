h: number;\n      percent_change_7d: number;\n      percent_change_12h: number;\n      percent_change_15m: number;\n      percent_change_24h: number;\n      percent_change_30d: number;\n      percent_change_30m: number;\n      percent_from_price_ath: number;\n      price: number;\n      volume_24h: number;\n      volume_24h_change_24h: number;\n    };\n  };\n}\n\ninterface PriceProps {\n  coinId: string;\n}\n\nfunction Price({ coinId }: PriceProps) {\n  const { isLoading, data: priceData } = useQuery<PriceData>(\n    [\"price\", coinId],\n    () => fetchCoinTickers(coinId),\n    { refetchInterval: 5000 }\n  );\n  return (\n    <div>\n      {isLoading ? (\n        \"Loading prices...\"\n      ) : (\n        <PriceList>\n          <PriceOne>\n            <span>Market Cap</span>\n            <span>${priceData?.quotes.USD.market_cap.toLocaleString()}</span>\n          </PriceOne>\n          <PriceOne>\n            <span>All Time High (Price)</span>\n            <span>${priceData?.quotes.USD.ath_price.toFixed(3)}</span>\n          </PriceOne>\n          <PriceOne>\n            <span>All Time High (Date)</span>\n            <span>{priceData?.quotes.USD.ath_date.toLocaleLowerCase()}</span>\n          </PriceOne>\n        </PriceList>\n      )}\n    </div>\n  );\n}\nexport default Price;\n","/Users/siabaek/Desktop/노마드코더/Crypto_Tracker/src/index.tsx",[],"/Users/siabaek/Desktop/노마드코더/Crypto_Tracker/src/theme.ts",[],"/Users/siabaek/Desktop/노마드코더/Crypto_Tracker/src/App.tsx",[],"/Users/siabaek/Desktop/노마드코더/Crypto_Tracker/src/Router.tsx",[],"/Users/siabaek/Desktop/노마드코더/Crypto_Tracker/src/routes/Coin.tsx",[],"/Users/siabaek/Desktop/노마드코더/Crypto_Tracker/src/routes/Coins.tsx",["83","84"],"/Users/siabaek/Desktop/노마드코더/Crypto_Tracker/src/api.ts",[],"/Users/siabaek/Desktop/노마드코더/Crypto_Tracker/src/routes/Chart.tsx",[],"/Users/siabaek/Desktop/노마드코더/Crypto_Tracker/src/routes/Price.tsx",["85"],{"ruleId":"86","replacedBy":"87"},{"ruleId":"88",